{
  "name": "ajv-cli",
  "version": "3.3.0",
  "description": "A command line interface for epoberezkin/ajv JSON schema validator",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublish": "npm run build",
    "eslint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "prettier:write": "prettier --write \"./**/*.{md,json,yaml,js,ts}\"",
    "prettier:check": "prettier --list-different \"./**/*.{md,json,yaml,js,ts}\"",
    "test-spec": "mocha test/*.spec.js -R spec",
    "test-cov": "nyc npm run test-spec",
    "test": "npm run build && npm run eslint && npm run test-cov"
  },
  "nyc": {
    "exclude": [
      "test",
      "node_modules"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "bin": {
    "ajv": "dist/index.js"
  },
  "preferGlobal": true,
  "author": "Jesse Collis <jesse@jcmultimedia.com.au>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessedc/ajv-cli"
  },
  "dependencies": {
    "ajv": "^6.7.0",
    "ajv-pack": "^0.3.0",
    "fast-json-patch": "^2.0.0",
    "glob": "^7.1.0",
    "js-yaml": "^3.13.1",
    "json-schema-migrate": "^0.2.0",
    "json5": "^2.1.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@ajv-validator/config": "^0.3.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "ajv-keywords": "^3.2.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "mocha": "^8.1.3",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "prettier": "@ajv-validator/config/prettierrc.json",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{md,json,yaml,js,ts}": "prettier --write"
  }
}
